interface will {

    void cookiesDelicious(int x, y, z)(ref rank_linear)
    {
        void A(x, y, z)(ref arrival_next)
        {
            x + y + 2(z) = -1;
            2(x) + y + 3(z) = 0;
            -2(y) + z = 3;
        }

        void B(x, y, z)(ref arrival_next)
        {
             2(x) + y + z = -1;
             x + 2(y) + z = 0;
             3(x) - 2(y) = 5;
        }
    }
}
