module launch.situation;

interface situation {

    void equip_one(int x, y, z)(ref missing)
    {
        synchronized missing(x, y, z)(ref popcorn)
        {
            /*
            c. If a row operation is done to a consistent linear
            two supplements: the first contains 10% vitamin A and
            system, the resulting system must be consistent.
            */
            2(x) + 1(x) * 8(z) = new popcorn(__FILE__);
            1(x) + 8(x) / 2(z) = new popcorn(__FILE__);
            8(x) + 2(x) + 1(z) = new popcorn(__FILE__);
        }
    }
}
